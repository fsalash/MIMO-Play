# .- Listar recetas
GET     /recipes                    controllers.RecipeController.retrieveRecipes

# Dar de alta una receta
POST     /recipe             controllers.RecipeController.createRecipe

#* .- Borrar receta
DELETE /recipe/:id          controllers.RecipeController.deleteRecipe(id:Integer)


#Ingredientes

# Dar de alta un ingrediente (body en formato JSON)
POST     /ingredient/:ingre             controllers.IngredientController.createIngredient(ingre:String)